	    event.returnValue = false;
	}
	 var id = parseUrl()[0];
	 var filename = "";
	  $.ajax({
		   url:'../openVideo',
		   data:{"fileId":id},
		   type:'get',
		   async: false,
		   success:function(data){
			   filename="../file"+data.substring(8,data.length);
				document.Intevueactivexctrl1.TIM_File_OpenFile(filename,'');
			//   console.log(data.indexOf("F:\\files"));
			   console.log(filename);
		   }
	   })
	 
	
	

	
})
 function parseUrl() {
				var url = location.href;
				var i = url.indexOf('?');
				if(i == -1) return;
				var querystr = url.substr(i + 1);
				var arr1 = querystr.split('&');
				var str = new Array();
				var num = 0;
				for(i in arr1) {
					var ta = arr1[i].split('=');
					str[num] = decodeURI(ta[1]);
					num++;
				}
				return str;
			}
</script>

