package ac.drsi.nestor.entity;

public class SVDS_Log {
	private Integer logId;// 日志Id
	private String operation;// 操作描述
	private String logDate;// 日期
	private SVDS_User user;// 用户对象
	private SVDS_Files file;// 文件对象
	private SVDS_Menu menu;// 菜单对象
	private String ipData;// ip地址
	private String result;// 执行结果

	/**
	 * 获取日志Id
	 * 
	 * @return
	 */
	public Integer getLogId() {
		return logId;
	}

	/**
	 * 设置日志Id
	 * 
	 * @param logId
	 *            日志Id
	 */
	public void setLogId(Integer logId) {
		this.logId = logId;
	}

	/**
	 * 获取日期
	 * 
	 * @return
	 */
	public String getLogDate() {
		return logDate;
	}

	/**
	 * 设置日期
	 * 
	 * @param logDate
	 *            日期
	 */
	public void setLogDate(String logDate) {
		this.logDate = logDate;
	}

	/**
	 * 获取用户对象
	 * 
	 * @return
	 */
	public SVDS_User getUser() {
		return user;
	}

	/**
	 * 设置用户对象
	 * 
	 * @param user
	 *            用户对象
	 */
	public void setUser(SVDS_User user) {
		this.user = user;
	}

	/**
	 * 无参构造方法
	 */
	public SVDS_Log() {
		super();
	}

	/**
	 * 日志对象的构造方法
	 * 
	 * @param operation
	 *            操作描述
	 * @param logDate
	 *            日期
	 * @param user
	 *            用户
	 * @param ipData
	 *            IP地址
	 * @param result
	 *            执行结果
	 */
	public SVDS_Log(String operation, String logDate, SVDS_User user,String ipData,String result) {
		super();
		this.operation = operation;
		this.logDate = logDate;
		this.user = user;
		this.ipData = ipData;
		this.result = result;
	}
//	public SVDS_Log(String operation, String logDate, SVDS_User user) {
//		super();
//		this.operation = operation;
//		this.logDate = logDate;
//		this.user = user;
//	}
	/**
	 * 日志对象的构造方法
	 * 
	 * @param operation
	 *            操作描述
	 * @param logDate
	 *            日期
	 * @param user
	 *            用户对象
	 * @param menu
	 *            菜单对象
	 * @param file
	 *            文件对象
	 * @param ipData
	 *            IP地址
	 * @param result
	 *            执行结果
	 */
	public SVDS_Log(String operation, String logDate, SVDS_User user,
			SVDS_Menu menu, SVDS_Files file,String ipData,String result) {
		super();
		this.operation = operation;
		this.logDate = logDate;
		this.user = user;
		this.menu = menu;
		this.file = file;
		this.ipData = ipData;
		this.result = result;
	}

	/**
	 * 获取操作描述
	 * 
	 * @return
	 */
	public String getOperation() {
		return operation;
	}

	/**
	 * 设置操作描述
	 * 
	 * @param operation
	 *            操作描述
	 */
	public void setOperation(String operation) {
		this.operation = operation;
	}

	/**
	 * 获取文件对象
	 * 
	 * @return
	 */
	public SVDS_Files getFile() {
		return file;
	}

	/**
	 * 获取菜单对象
	 * 
	 * @return
	 */
	public SVDS_Menu getMenu() {
		return menu;
	}

	/**
	 * 设置菜单对象
	 * 
	 * @param menu
	 *            菜单对象
	 */
	public void setMenu(SVDS_Menu menu) {
		this.menu = menu;
	}

	/**
	 * 设置文件对象
	 * 
	 * @param file
	 *            文件对象
	 */
	public void setFile(SVDS_Files file) {
		this.file = file;
	}

	/**
	 * 获取IP地址
	 * 
	 * @return
	 */
	public String getIpData() {
		return ipData;
	}

	/**
	 * 设置IP地址
	 * 
	 * @param ipData
	 *            IP地址
	 */
	public void setIpData(String ipData) {
		this.ipData = ipData;
	}

	/**
	 * 获取执行结果
	 * 
	 * @return
	 */
	public String getResult() {
		return result;
	}

	/**
	 * 设置执行结果
	 * 
	 * @param result
	 *            执行结果
	 */
	public void setResult(String result) {
		this.result = result;
	}

}
